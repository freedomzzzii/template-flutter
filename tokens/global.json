  "fontSizesEN": {
    "base": {
      "value": "{fontBase}",
      "type": "fontSizes"
    },
    "scaleUp1": {
      "value": "roundTo({fontSizesEN.base}*{fontScale})",
      "type": "fontSizes"
    },
    "scaleUp2": {
      "value": "roundTo( {fontSizesEN.scaleUp1}*{fontScale})",
      "type": "fontSizes"
    },
    "scaleUp3": {
      "value": "roundTo( {fontSizesEN.scaleUp2}*{fontScale})",
      "type": "fontSizes"
    },
    "scaleDown1": {
      "value": "roundTo( {fontSizesEN.base} / {fontScale})",
      "type": "fontSizes"
    },
    "scaleDown2": {
      "value": "roundTo( {fontSizesEN.scaleDown1}/{fontScale})+1",
      "type": "fontSizes"
    },
    "scaleDown3": {
      "value": "roundTo( {fontSizesEN.scaleDown2}/{fontScale})",
      "type": "fontSizes"
    }
  },
  "fontSizesTH": {
